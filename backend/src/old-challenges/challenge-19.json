    {
      "input": [1, 2, 3, 4, 5],
      "expectedOutput": 10
    },
    {
      "input": [54, 11, 58],
      "expectedOutput": 2
    },
    {
      "input": [10, 20, 40, 2, 11],
      "expectedOutput": 5
    },
    {
      "input": [0, 68, 75, 34, 89, 66, 76, 72, 25],
      "expectedOutput": 20
    },
    {
      "input": [60, 64, 94, 30, 93, 84, 76, 16, 45, 0, 0, 57, 80],
      "expectedOutput": 28
    },
    {
      "input": [75],
      "expectedOutput": 0
    },
    {
