{
    "tests": [
        {
            "input": {"count": 2, "votes": [[1, 2], [1, -2], [-2, -1]]},
            "expectedOutput": [1, 0]
        },
        {
            "input": {"count": 3, "votes": [[1, 2], [2, 3], [3, -2], [1, -3]]},
            "expectedOutput": [1, 1, 0]
        },
        {
            "input": {"count": 4, "votes": [[-1, -2], [-2, -3], [-3, -4], [-4, -1]]},
            "expectedOutput": [0, 0, 0, 0]
        },
        {
            "input": {"count": 2, "votes": [[1, 2], [-1, -2]]},
            "expectedOutput": [1, 1]
        },
        {
            "input": {"count": 3, "votes": [[1, -3], [-2, 3], [1, -2]]},
            "expectedOutput": [1, -1, 1]
        }
    ]
}