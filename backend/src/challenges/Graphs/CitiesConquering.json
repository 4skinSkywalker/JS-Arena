{
    "tests": [
        {
            "input": {"n": 10, "roads": [[1, 0], [1, 2], [8, 5], [9, 7], [5, 6], [5, 4], [4, 6], [6, 7]]},
            "expectedOutput": [1, 2, 1, 1, -1, -1, -1, 2, 1, 1]
        },
        {
            "input": {"n": 4, "roads": [[0, 1], [1, 2], [2, 3]]},
            "expectedOutput": [1, 2, 2, 1]
        },
        {
            "input": {"n": 5, "roads": [[0, 1], [2, 3], [2, 4]]},
            "expectedOutput": [1, 1, 1, 1, 1]
        },
        {
            "input": {"n": 6, "roads": [[0, 1], [1, 2], [2, 3], [3, 4], [4, 5]]},
            "expectedOutput": [1, 2, 3, 3, 2, 1]
        },
        {
            "input": {"n": 3, "roads": [[0, 1], [1, 2]]},
            "expectedOutput": [1, 2, 1]
        }
    ]
}