{
    "tests": [
        {
            "input": {
                "size": 5, 
                "nonogramField": [
                    ["-", "-", "-", "-", "-", "-", "-", "-"],
                    ["-", "-", "-", "2", "2", "1", "-", "1"],
                    ["-", "-", "-", "2", "1", "1", "3", "3"],
                    ["-", "3", "1", "#", "#", "#", ".", "#"],
                    ["-", "-", "2", "#", "#", ".", ".", "."],
                    ["-", "-", "2", ".", ".", ".", "#", "#"],
                    ["-", "1", "2", "#", ".", ".", "#", "#"],
                    ["-", "-", "5", "#", "#", "#", "#", "#"]
                ]
            },
            "expectedOutput": true
        },
        {
            "input": {
                "size": 5, 
                "nonogramField": [
                    ["-", "-", "-", "-", "-", "-", "-", "-"],
                    ["-", "-", "-", "-", "-", "1", "-", "-"],
                    ["-", "-", "-", "3", "3", "2", "5", "5"],
                    ["-", "-", "3", ".", ".", ".", "#", "#"],
                    ["-", "2", "2", "#", "#", "#", "#", "#"],
                    ["-", "-", "5", "#", "#", "#", "#", "#"],
                    ["-", "-", "5", "#", "#", "#", "#", "#"],
                    ["-", "-", "2", ".", ".", ".", "#", "#"]
                ]
            },
            "expectedOutput": false
        },
        {
            "input": {
                "size": 4, 
                "nonogramField": [
                    ["-", "-", "-", "-", "-", "-"],
                    ["-", "-", "1", "2", "4", "4"],
                    ["-", "3", "1", "#", "#", "#"],
                    ["-", "2", "2", "#", "#", ".", "."],
                    ["-", "4", "#", "#", "#", "#"]
                ]
            },
            "expectedOutput": true
        },
        {
            "input": {
                "size": 6, 
                "nonogramField": [
                    ["-", "-", "-", "-", "-", "-", "-", "-"],
                    ["-", "-", "-", "3", "2", "1", "-", "1"],
                    ["-", "-", "-", "2", "1", "1", "3", "3"],
                    ["-", "3", "1", "#", "#", "#", ".", "#"],
                    ["-", "-", "2", "#", "#", ".", ".", "."],
                    ["-", "-", "2", ".", ".", ".", "#", "#"],
                    ["-", "1", "2", "#", ".", ".", "#", "#"],
                    ["-", "-", "5", "#", "#", "#", "#", "#"]
                ]
            },
            "expectedOutput": false
        },
        {
            "input": {
                "size": 1, 
                "nonogramField": [
                    ["-", "-", "-"],
                    ["-", "1", "#"]
                ]
            },
            "expectedOutput": true
        }
    ]
}