{
    "tests": [
        {
            "input": {"l1": [1, 2, 3], "l2": [4, 5, 6]},
            "expectedOutput": [1, 2, 3, 4, 5, 6]
        },
        {
            "input": {"l1": [1, 1, 2, 4], "l2": [0, 3, 5]},
            "expectedOutput": [0, 1, 1, 2, 3, 4, 5]
        },
        {
            "input": {"l1": [], "l2": [1, 2, 3]},
            "expectedOutput": [1, 2, 3]
        },
        {
            "input": { "l1": [2, 3, 4], "l2": []},
            "expectedOutput": [2, 3, 4]
        },
        {
            "input": { "l1": [5, 10, 15], "l2": [2, 3, 20]},
            "expectedOutput": [2, 3, 5, 10, 15, 20]
        }
    ]
}