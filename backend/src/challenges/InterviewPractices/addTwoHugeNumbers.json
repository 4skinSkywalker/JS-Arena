            "input": {"a": [9876, 5432, 1999], "b": [1, 8001]},
            "expectedOutput": [9876, 5434, 0]
        },
        {
            "input": {"a": [123, 4, 5], "b": [100, 100, 100]},
            "expectedOutput": [223, 104, 105]
        },
        {
            "input": {"a": [1111, 2222, 3333], "b": [4444, 5555, 6666]},
            "expectedOutput": [5555, 7777, 9999]
        },
        {
            "input": {"a": [9999, 9999, 9999], "b": [1, 0, 0]},
